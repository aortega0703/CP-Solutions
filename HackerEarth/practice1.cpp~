#include <bits/stdc++.h>
#define DEBUG
#ifdef DEBUG
#define Debug(x) cout << x
#else
#define Debug(x)
#endif

using namespace std;

typedef long long ll;
typedef pair<int, int> ii;

const int INF = 1 << 30;

int gcd(a, b){
  return b? gcd(b, a%b) : a;
}

int main(){
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);
  int a, int b;
  cin >> a >> b;
  int i = 0;
  int c = gcd(a,b);
  while(c != 1){
    ++i;
    c = gcd(a,b);
  }
  cout << i << "\n";
}
